@model signalrtc.DataAccess.SIPAccount

@{
    ViewData["Title"] = "Reset Password";
}

<h1>Reset Password</h1>

<h4>SIP Account</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                @Html.DisplayNameFor(model => model.AOR)
                @Html.DisplayFor(model => model.AOR)
            </div>
            <div class="form-group">
                @Html.DisplayNameFor(model => model.SIPPassword)
                <input asp-for="SIPPassword" class="form-control" />
                <span asp-validation-for="SIPPassword" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Account">Back to Your Account</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
