@model devcall.DataAccess.SIPAccount

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>SIPAccount</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ID" class="control-label"></label>
                <input asp-for="ID" class="form-control" />
                <span asp-validation-for="ID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DomainID" class="control-label"></label>
                <select asp-for="DomainID" class ="form-control" asp-items="ViewBag.DomainID"></select>
            </div>
            <div class="form-group">
                <label asp-for="SIPDialPlanID" class="control-label"></label>
                <select asp-for="SIPDialPlanID" class ="form-control" asp-items="ViewBag.SIPDialPlanID"></select>
            </div>
            <div class="form-group">
                <label asp-for="SIPUsername" class="control-label"></label>
                <input asp-for="SIPUsername" class="form-control" />
                <span asp-validation-for="SIPUsername" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="SIPPassword" class="control-label"></label>
                <input asp-for="SIPPassword" class="form-control" />
                <span asp-validation-for="SIPPassword" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="IsDisabled" /> @Html.DisplayNameFor(model => model.IsDisabled)
                </label>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
